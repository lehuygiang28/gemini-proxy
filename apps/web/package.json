{
    "name": "@gemini-proxy/web",
    "version": "0.1.0",
    "private": true,
    "description": "Gemini Proxy Web Application - API Key Management System",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --no-deprecation\" PORT=4040 refine dev",
        "build": "cross-env NODE_OPTIONS=\"--no-deprecation\" refine build",
        "start": "cross-env NODE_ENV=production PORT=4041 refine start",
        "start:dev": "cross-env NODE_ENV=development PORT=4040 refine start",
        "lint": "next lint",
        "refine": "refine",
        "clean": "rm -rf .next .turbo"
    },
    "dependencies": {
        "@ant-design/icons": "^5.5.1",
        "@ant-design/nextjs-registry": "^1.0.0",
        "@gemini-proxy/core": "workspace:*",
        "@gemini-proxy/vercel": "workspace:*",
        "@refinedev/antd": "^5.44.0",
        "@refinedev/cli": "^2.16.21",
        "@refinedev/core": "^4.47.1",
        "@refinedev/devtools": "^1.2.17",
        "@refinedev/kbar": "^1.3.17",
        "@refinedev/nextjs-router": "^6.0.0",
        "@refinedev/supabase": "^5.7.4",
        "@supabase/ssr": "^0.3.0",
        "antd": "^5.17.0",
        "gsap": "^3.13.0",
        "js-cookie": "^3.0.5",
        "motion": "^12.23.12",
        "next": "^14.2.0",
        "ogl": "^1.0.11",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-icons": "^5.2.1",
        "react-syntax-highlighter": "^15.5.0"
    },
    "devDependencies": {
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^18.19.123",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "cross-env": "^7.0.3",
        "eslint": "^8",
        "eslint-config-next": "^14.2.31",
        "typescript": "^5.9.2"
    }
}
