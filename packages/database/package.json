{
    "name": "@gemini-proxy/database",
    "version": "0.0.1",
    "type": "module",
    "description": "Database schema/types for Gemini Proxy",
    "scripts": {
        "build": "tsup",
        "gen:types": "npx @dotenvx/dotenvx run -f ./.env -- node ./scripts/gen-types.mjs",
        "db:push": "npx @dotenvx/dotenvx run -f ./.env -- node ./scripts/db-push.mjs",
        "apply:stats": "npx @dotenvx/dotenvx run -f ./.env -- node ./scripts/apply-statistics-functions.mjs"
    },
    "license": "MIT",
    "devDependencies": {
        "@supabase/supabase-js": "^2.55.0",
        "cross-env": "^7.0.3",
        "rimraf": "^6.0.1",
        "supabase": "^2.39.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
